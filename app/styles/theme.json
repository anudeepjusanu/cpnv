{
    "palette": {
        "primary": {
            "main": "#000000"
        },
        "secondary": {
            "main": "#127AC2",
            "contrastText": "#ffffff"
        }
    },
    "typography": {
        "fontFamily": "Arial",
        "body1": {
            "fontSize": "14px",
            "lineHeight": 1
        },
        "h3": {
            "fontSize": "21px",
            "fontWeight": "bold"
        },
        "h4": {
            "fontSize": "18px",
            "fontWeight": "bold"
        },
        "h5": {
            "fontSize": "15px",
            "fontWeight": "bold"
        },
        "overline": {
            "fontSize": "11px",
            "opacity": "0.5",
            "letterSpacing": "1.1px"
        }
    },
    "overrides": {
        "MuiFormLabel": {
            "root": {
                "fontSize": "13px",
                "lineHeight": "1.5"
            }
        },
        "MuiOutlinedInput": {
            "inputMarginDense": {
                "paddingTop": "11.7px",
                "paddingBottom": "11.7px"
            }
        },
        "MuiStepLabel": {
            "label": {
                "&$active": {
                    "color": "#000000",
                    "fontWeight": "bold"
                }
            }
        },
        "MuiSelect": {
            "select": {
                "&:focus": {
                    "backgroundColor": "transparent"
                }
            },
            "outlined": {
                "paddingTop": "11.7px",
                "paddingBottom": "11.7px"
            }
        },
        "MuiInputLabel": {
            "outlined": {
                "transform": "translate(14px, 12px) scale(1)"
            }
        },
        "MuiStepIcon": {
            "root": {
                "&$completed": {
                    "color": "#127AC2"
                },
                "&$active": {
                    "color": "#127AC2"
                }
            }
        },
        "MUIDataTableToolbar": {
            "root": {
                "padding": "0"
            }
        },
        "MUIDataTableHeadCell": {
            "data": {
                "font-size": "13px",
                "font-weight": "bold"
            }
        },
        "MUIDataTablePagination": {
            "tableCellContainer": {
                "padding": "0"
            }
        },
        "MuiTableCell": {
            "head": {
                "padding": "10px",
                "border": "none"
            },
            "root": {
                "font-size": "13px",
                "padding": "10px",
                "border": "none"
            }
        },
        "MuiTablePagination": {
            "root": {
                "width": "100%"
            },
            "toolbar": {
                "padding": 0
            },
            "spacer": {
                "flexGrow": "0",
                "flexShrink": "0",
                "flexBasis": "0"
            },
            "actions": {
                "flexGrow": "1",
                "flexShrink": "1",
                "flexBasis": "100%",
                "display": "flex",
                "justifyContent": "flex-end"
            }
        },
        "MuiDrawer": {
            "paper": {
                "background": "none",
                "color": "#ffffff"
            }
        }
    }
}